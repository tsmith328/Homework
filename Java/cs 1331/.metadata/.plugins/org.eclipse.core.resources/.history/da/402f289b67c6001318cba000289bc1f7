
public class MyStack<T> {
    private Node top;
    private Node bottom;

    public MyStack() {
       top = null;
       bottom = null;
    }

    public void push(T elmt) {
        Node n = new Node(elmt, top);
        top = n;
    }

    public T pop() {
        T element = top.me;
        Node next = top.next;
        top = next;
        return element;
    }

    public boolean isEmpty() {
        if (top == bottom) {
            return true;
        }
        return false;
    }
    private class Node {
        private T me;
        private Node next;
        public Node(T elem, Node next) {
            me = elem;
            this.next = null;
        }
    }
}
