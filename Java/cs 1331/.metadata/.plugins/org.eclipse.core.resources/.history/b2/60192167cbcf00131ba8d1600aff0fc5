import java.util.Scanner;


public class FinalPractice {
    public static void main(String[] args) {
        
    }
    
    public static int howOften(String word, String sentence) {
        int count = 0;
        Scanner s = new Scanner(sentence);
        while (s.hasNext()) {
            if ((s.next()).equals(word)) {
                count++;
            }
        }
        return count;
    }
}
